<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/sitemesh">

	<excludes>
		<pattern>*Proc*</pattern>
		<pattern>*.ajax</pattern>
	</excludes>

	<decorator name="main" page="main/main.jsp">
		<pattern>/main/main.do</pattern>
	</decorator>

	<decorator name="sub" page="main/main.jsp">
		<pattern>/*</pattern>
	</decorator>

</decorators>